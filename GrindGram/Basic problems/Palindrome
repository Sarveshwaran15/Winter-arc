Input: n = 555
Output: true

class Solution {
    public boolean isPalindrome(int n) {
        // code here
        int rev=0;
        int value=n;
        while(n>0)
        {
            int digit=n%10;
            rev=(rev*10)+digit;
            n/=10;
        }
        if(value==rev)
            return true;
        return false;
    }
}
