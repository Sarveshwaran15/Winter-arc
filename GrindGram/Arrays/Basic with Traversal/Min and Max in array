Input: arr[] = [1, 4, 3, -5, -4, 8, 6]
Output: [-5, 8]

class Solution {
    public ArrayList<Integer> getMinMax(int[] arr) {
        // code Here
        ArrayList<Integer> result=new ArrayList<>();
        Arrays.sort(arr);
        result.add(arr[0]);
        result.add(arr[arr.length-1]);
        return result;
    }
}
