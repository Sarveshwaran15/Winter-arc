Input: a[] = [1, 2, 5, 4, 0], b[] = [2, 4, 5, 0, 1]
Output: true

class Solution {
    public static boolean checkEqual(int[] a, int[] b) {
        // code here
        Arrays.sort(a);
        Arrays.sort(b);
        int i=0;
        while(i<a.length)
        {
            if(a[i]!=b[i])
                return false;
            i++;
        }
        return true;
    }
}
