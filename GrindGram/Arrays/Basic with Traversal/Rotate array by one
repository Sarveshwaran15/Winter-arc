Input: arr[] = [1, 2, 3, 4, 5]
Output: [5, 1, 2, 3, 4]

// // User function Template for Java

class Solution {
    public void rotate(int[] arr) {
        // code here
       int temp=arr[arr.length-1];
       for (int i = arr.length - 1; i > 0; i--) {
            arr[i] = arr[i - 1];
        }
        arr[0]=temp;
    }
}
